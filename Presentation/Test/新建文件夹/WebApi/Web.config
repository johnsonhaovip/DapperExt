  </configSections>
  <XNYConfig>
    <DynamicDiscovery Enabled="true" />
    <Engine Type="XNY.Helper.Infrastructure.WebApiEngine" />
    <Themes basePath="~/Themes/drak/" />
    <UserAgentStrings databasePath="~/App_Data/uas_20180723-01.ini" />
    <IsResolve Enabled="false"/>
  </XNYConfig>

  <appSettings>
    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
    <!--日志配置-->
    <add key="logConfigPath" value="~/configs/log4net.config" />
  </appSettings>
  <connectionStrings>
    <add name="Test" providerName="System.Data.SqlClient" connectionString="server=127.0.0.1;uid=sa;pwd=123;database=Student"/>
  </connectionStrings>
  
  <!--
